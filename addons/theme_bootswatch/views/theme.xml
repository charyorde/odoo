    <template id="theme_bootswatch.account_verify" name="Account verify">
      <t t-call="web.login_layout">
          <!--xpath expr="." position="inside">
          <link rel="stylesheet" type="text/less" href="/theme_bootswatch/static/src/css/jquery.fileupload.css"/>
          <script src="/theme_bootswatch/static/src/js/app.js" type="text/javascript"/>
          <script src="/theme_bootswatch/static/src/js/jquery.ui.widget.js" type="text/javascript"/>
          <script src="/theme_bootswatch/static/src/js/jquery.iframe-transport.js" type="text/javascript"/>
          <script src="/theme_bootswatch/static/src/js/jquery.fileupload.js" type="text/javascript"/>
        </xpath-->
        <h1>Profile Verification</h1>
        <form role="form" t-attf-action="#" method="POST" enctype="multipart/form-data" onsubmit="this.action = this.action + location.hash">
          <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
          <div id="account_type" class="btn-group">
            <label class="btn btn-default active"><input type="radio" name="company_type" value="person" checked="checked"/> Individual
                </label>
            <label class="btn btn-default"><input type="radio" name="company_type" value="company"/> Corporate
                </label>
          </div>
          <div class="form-group field-bvn">
            <label for="bvn" class="control-label">BVN</label>
            <input type="text" name="bvn" t-att-value="bvn" id="bvn" class="form-control" required="required" autofocus="autofocus"/>
          </div>
          <div id="identity-wrapper">
            <div class="form-group field-identity-id ind">
                <input t-att-value="identity_id" class="inputfile inputfile-1" id="identity_id" type="file" name="identity_id" data-url="/file/upload/identity_id/gwtemp" data-gw-files="{count} file(s) attached" autofocus="autofocus"/>
              <label for="identity_id"><span>Upload Your Identity ID</span></label>
            </div>
            <div id="spinner-identity"/>
            <div id="identity-status" class="file-status"><ul></ul></div>
            <input type="hidden" id="gw_idfn" name="gw_idfn" t-att-value="gw_idfn"/>
          </div>
          <div class="form-group field-empname ind">
            <label for="empname" class="control-label">Employer name</label>
            <input type="text" name="empname" t-att-value="empname" id="empname" class="form-control" required="required" autofocus="autofocus"/>
          </div>
          <div class="form-group field-companyname corp">
            <label for="companyname" class="control-label">Company name</label>
            <input type="text" name="companyname" t-att-value="companyname" id="companyname" class="form-control" required="required" autofocus="autofocus"/>
          </div>
          <div class="form-group field-mexpenses ind">
            <label for="mexpenses" class="control-label">Total monthly expenses</label>
            <input type="text" name="mexpenses" t-att-value="mexpenses" id="mexpenses" class="form-control" required="required" autofocus="autofocus"/>
          </div>
        <div class="clearfix">
          <div class="form-group field-payslips ind">
              <input t-att-value="payslips" class="inputfile inputfiles" id="payslips" type="file" name="payslips" data-url="/files/upload/payslips" data-gw-files="{count} file(s) attached" autofocus="autofocus" multiple="multiple"/>
            <label for="payslips"><span>Upload Your Latest Payslips</span></label>
          </div>
          <div id="spinner-payslips"/>
          <div id="payslips-status" class="file-status"><ul></ul></div>
            <input type="hidden" id="gw_pyslpfn" name="gw_pyslpfn" t-att-value="gw_pyslpfn"/>
      </div>
          <div class="form-group field-company-regid corp">
            <label for="company_reg_id" class="control-label">Company Reg Id</label>
            <input type="text" name="company_reg_id" t-att-value="company_reg_id" id="company_reg_id" class="form-control" required="required" autofocus="autofocus"/>
          </div>
          <div class="form-group field-address">
            <label for="address" class="control-label">Address</label>
            <input type="text" name="address" t-att-value="address" id="address" class="form-control" required="required" autofocus="autofocus"/>
        </div>
        <div class="clearfix">
          <div class="form-group field-tenancy ind">
              <input t-att-value="tenancy" class="inputfile inputfile-1" id="tenancy" type="file" name="tenancy" data-url="/file/upload/tenancy/gwtemp"  data-gw-files="{count} file(s) attached" autofocus="autofocus"/>
            <label for="tenancy"><span>Upload Your Tenancy Agreement</span></label>
          </div>
          <div id="spinner-tenancy"/>
          <div id="tenancy-status" class="file-status"><ul></ul></div>
            <input type="hidden" id="gw_tncyfn" name="gw_tncyfn" t-att-value="gw_tncyfn"/>
      </div>
          <div class="form-group field-job-position">
            <label for="job_position" class="control-label">Job position</label>
            <input type="text" name="job_position" t-att-value="job_position" id="job_position" class="form-control" required="required" autofocus="autofocus"/>
          </div>
          <input type="hidden" name="cyear" t-att-value="cyear"/>
          <t t-log="months_short"/>
          <div class="form-group field-debit-date">
            <label for="debit_date" class="control-label">Debit date</label>
            <div style="display: inline-flex">
              <select id="gw-mths-list" class="form-control" name="debit_date_month" required="required" autofocus="autofocus">
                <t t-foreach="months_short" t-as="item">
                  <option t-att-value="item_value[0]">
                    <t t-esc="item_value[1]"/>
                  </option>
                </t>
              </select>
              <select id="gw-days-list" class="form-control" name="debit_date_day" required="required" autofocus="autofocus">
                <t t-foreach="days" t-as="day">
                  <option t-att-value="day_value">
                    <t t-esc="day_value"/>
                  </option>
                </t>
              </select>
            </div>
        </div>
          <p class="alert alert-danger" t-if="error">
            <t t-esc="error"/>
          </p>
          <p class="alert alert-success" t-if="message">
            <t t-esc="message"/>
        </p>
          <input type="hidden" name="redirect" t-att-value="redirect"/>
          <div class="clearfix oe_login_buttons">
            <button type="submit" onclick="GW.formValidate()" formnovalidate="formnovalidate" class="btn btn-primary">Submit</button>
          </div>
        </form>
      </t>
    </template>
    <template id="theme_bootswatch.profile_edit_person" name="Profile edit person">
      <t t-call="web.login_layout">
        <xpath expr="." position="inside">
          <script src="/theme_bootswatch/static/src/js/app.js" type="text/javascript"/>
          <script src="/theme_bootswatch/static/src/js/jquery.ui.widget.js" type="text/javascript"/>
          <script src="/theme_bootswatch/static/src/js/jquery.iframe-transport.js" type="text/javascript"/>
          <script src="/theme_bootswatch/static/src/js/jquery.fileupload.js" type="text/javascript"/>
        </xpath>
        <h1>Edit Profile</h1>
        <form role="form" t-attf-action="/profile/{{userid}}/edit" method="POST" enctype="multipart/form-data" onsubmit="this.action = this.action + location.hash">
          <div class="form-group field-bvn">
            <label for="bvn" class="control-label">BVN</label>
            <input type="text" class="form-control" required="required" autofocus="autofocus" name="bvn" id="bvn" t-att-value="bvn"/>
          </div>
          <div class="form-group field-empname">
            <label for="empname" class="control-label">Employer</label>
            <input type="text" name="empname" id="empname" class="form-control" t-att-value="empname" required="required" autofocus="autofocus"/>
          </div>
          <div class="form-group field-mexpenses">
            <label for="mexpenses" class="control-label">Total monthly expenses</label>
            <input type="text" name="mexpenses" id="mexpenses" class="form-control" required="required" autofocus="autofocus" t-att-value="mexpenses"/>
          </div>
          <div class="form-group field-address">
            <label for="address" class="control-label">Address</label>
            <input type="text" name="address" id="address" class="form-control" required="required" autofocus="autofocus" t-att-value="address"/>
          </div>
          <div class="form-group field-job-position">
            <label for="job_position" class="control-label">Job position</label>
            <input type="text" name="job_position" id="job_position" class="form-control" t-att-value="job_position" required="required" autofocus="autofocus"/>
          </div>
          <div class="clearfix oe_login_buttons">
            <button type="submit" class="btn btn-primary">Update</button>
          </div>
        </form>
      </t>
    </template>
