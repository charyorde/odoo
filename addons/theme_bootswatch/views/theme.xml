    <template id="theme_bootswatch.profile" name="Profile">
      <t t-call="web.login_layout">
        <h1>Profile
            <span style="float: right">
                <button class="btn btn-sm" type="button"><t t-esc="verb"/></button>
            </span>
        </h1>
        <div id="corp-container">
          <div class="gw-field-view" style="display: inline-block; min-height: 50px; max-height:120px">
            <label style="width: 200px">BVN</label>
            <span t-field="profile.bvn" t-field-options="{&quot;widget&quot;: &quot;text&quot;}"/>
          </div>
          <div class="gw-field-view" style="display: inline-block; min-height: 50px; max-height: 120px">
              <label style="width: 200px">Employer</label>
            <span t-field="profile.empname" t-field-options="{&quot;widget&quot;: &quot;text&quot;}"/>
          </div>
          <div class="gw-field-view">
            <label>Address</label>
            <span t-field="profile.street" t-field-options="{&quot;widget&quot;: &quot;text&quot;}"/>
          </div>
          <div class="gw-field-view">
            <label>Phone</label>
            <span t-field="profile.phone" t-field-options="{&quot;widget&quot;: &quot;phone&quot;}"/>
          </div>
          <div class="gw-field-view">
              <label>Total monthly expenses</label>
            <span t-field="profile.mexpenses" t-field-options="{&quot;widget&quot;: &quot;text&quot;}"/>
          </div>
          <div class="gw-field-view">
            <label>Job Position</label>
            <span t-field="profile.function" t-field-options="{&quot;widget&quot;: &quot;text&quot;}"/>
          </div>
          <div class="gw-field-view">
            <label>Debit date</label>
            <span t-field="profile.debit_date" t-field-options="{&quot;format&quot;: &quot;Y-m-d&quot;}"/>
          </div>
        </div>
