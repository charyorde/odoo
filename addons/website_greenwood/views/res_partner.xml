<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="1">
    <record model="ir.ui.view" id="view_partner_greenwood_profile_form">
      <field name="name">res.partner.form.greenwood.inherit</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="view_type">tree,form</field>
      <field name="arch" type="xml">
        <page name="accounting" position="after">
          <page string="Greenwood Profile" name="gwprofile">
            <group>
              <group string="Profile" name="gw_profile">
                <field name="empname" attrs="{'invisible': [('is_company','=', True)]}"/>
                <field name="companyname" attrs="{'invisible': [('is_company','=', False)]}"/>
                <field name="mexpenses" attrs="{'invisible': [('is_company','=', True)]}"/>
                <field name="company_reg_id" attrs="{'invisible': [('is_company','=', False)]}"/>
                <field name="debit_date" />
              </group>
            </group>
          </page>
        </page>
      </field>
    </record>
  </data>
</openerp>
